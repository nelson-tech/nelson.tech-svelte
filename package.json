{
	"name": "nelson.tech",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"schema": "graphql-codegen --require dotenv/config --config ./packages/wp/graphql/getSchema.yaml",
		"gen": "graphql-codegen --config ./packages/wp/graphql/codegen.yaml",
		"start": "node dist/index.js -p 3000",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"clean": "rm -rf dist .svelte-kit node_modules",
		"test": "playwright test",
		"jest": "jest src/tests",
		"jest:watch": "yarn jest --watch",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@graphql-typed-document-node/core": "^3.1.1",
		"@kitql/client": "^0.5.7",
		"@kitql/graphql-codegen": "^0.5.2",
		"@kitql/vite-plugin-watch-and-run": "^0.3.4",
		"@playwright/test": "^1.21.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "^1.0.0-next.78",
		"@sveltejs/kit": "next",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/svelte": "^3.1.2",
		"@types/cookie": "^0.5.1",
		"@types/jest": "^27.5.1",
		"@types/js-cookie": "^3.0.2",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.7",
		"cookie": "^0.5.0",
		"dotenv": "^16.0.1",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"graphql-tag": "^2.12.6",
		"jest": "^28.1.0",
		"jest-environment-jsdom": "^28.1.0",
		"js-cookie": "^3.0.1",
		"postcss": "^8.4.14",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-jester": "^2.3.2",
		"svelte-preprocess": "^4.10.6",
		"tailwindcss": "^3.0.24",
		"ts-jest": "^28.0.3",
		"tslib": "^2.3.1",
		"typescript": "~4.7.2",
		"uuid": "^8.3.2"
	},
	"type": "module",
	"license": "UNLICENSED",
	"dependencies": {
		"svelte-i18n": "^3.4.0"
	}
}
